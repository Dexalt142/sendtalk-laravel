{
    "name": "dexalt142/sendtalk",
    "description": "Unofficial TapTalk's SendTalk library for Laravel.",
    "keywords": ["laravel", "taptalk", "sendtalk"],
    "type": "library",
    "license": "MIT",
    "version": "1.0",
    "homepage": "https://github.com/Dexalt142/sendtalk-laravel",
    "authors": [
        {
            "name": "Dexalt142",
            "email": "faturrhmns14@gmail.com",
            "homepage": "https://github.com/Dexalt142"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Dexalt142\\SendTalk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dexalt142\\SendTalk\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dexalt142\\SendTalk\\ServiceProvider"
            ],
            "aliases": {
                "SendTalk": "Dexalt142\\SendTalk\\Facades\\SendTalk"
            }
        }
    }
}
